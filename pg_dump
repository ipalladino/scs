--
-- Name: generic_items; Type: TABLE; Schema: public; Owner: ignacio; Tablespace: 
--

CREATE TABLE generic_items (
    id integer NOT NULL,
    title character varying(255),
    description text,
    graphic character varying(255),
    button_link character varying(255),
    button_text character varying(255),
    created_at timestamp without time zone,
    updated_at timestamp without time zone,
    component integer,
    parent_item_id integer,
    is_default boolean DEFAULT false NOT NULL,
    priority integer DEFAULT 0,
    background_color character varying(255) DEFAULT 'transparent'::character varying,
    color character varying(255) DEFAULT '#000'::character varying
);


ALTER TABLE public.generic_items OWNER TO ignacio;

--
-- Name: generic_items_id_seq; Type: SEQUENCE; Schema: public; Owner: ignacio
--

CREATE SEQUENCE generic_items_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.generic_items_id_seq OWNER TO ignacio;

--
-- Name: generic_items_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: ignacio
--

ALTER SEQUENCE generic_items_id_seq OWNED BY generic_items.id;


